--This subquery looks at the FHA_ACCOUNT tree for P&amp;L accounts
Select
CAST(rtrim(ACCOUNT) AS NVARCHAR(30)) [GL Account]
,CAST(rtrim(PH_NODE_LVL4) AS NVARCHAR(50)) [Level 4 Account Code]
,CAST(rtrim(PH_NODE_LVL4_DESCR) AS NVARCHAR(100)) [Level 4 Account Name]
from
	PS_PH_ACCT_VCH_FH A
where
	SETID = 'SET00'
	and TREE_NAME = 'FHA_ACCOUNT'
	and ACCOUNT not like '0%'
	and EFFDT &lt;= (select (max(A_ED.EFFDT)) from PS_PH_TR_CC_VCH_FH A_ED where A.SETID = A_ED.SETID and A.TREE_NAME = A_ED.TREE_NAME --2023/02/10 ensure only one tree is returned if the PS_PH_TR_CC_VCH_RG view contains more than one effective date
	)	--2023/04/04 updated to rely on tree table which selects the tree based on current date

union all
--This subquery looks at the FHA_ACCOUNT tree for B/S accounts
