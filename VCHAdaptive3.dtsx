              SQLTask:SqlStatementSource="/***************&#xA;4.3.2.3. FILE REF NO. 22 – ACTUALS WITH COC, PROJECT ID AND SUBPROJECT&#xA;Filename: 22_actuals_capital.csv&#xA;Data Source: FSCM&#xA;Original story: FAA-10366&#xA;Updated: 2023/04/12&#xA;****************/&#xA;&#xA;--prep temp tables&#xA;&#xA;IF OBJECT_ID(N'tempdb..##PID_Balances') IS NOT NULL&#xA;BEGIN&#xA;DROP TABLE ##PID_Balances&#xA;END&#xA;&#xA;IF OBJECT_ID(N'tempdb..##PID_Dummies') IS NOT NULL&#xA;BEGIN&#xA;DROP TABLE ##PID_Dummies&#xA;END&#xA;&#xA;declare @fy as varchar(4)&#xA;declare @per as varchar(2)&#xA;--declare @PID as varchar(6) --for testing&#xA;&#xA;SET @fy = &#xA; (select FISCAL_YEAR from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in&#xA; (select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))&#xA;SET @per =&#xA; (select ACCOUNTING_PERIOD from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in&#xA; (select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))&#xA;--set @PID = '%' --for testing 132279 AM tsf, 132256 Ops tsf&#xA;&#xA;--Temp table 1 ##PID_Balances for data&#xA;--The following unions contain 5 subqueries for different elements of the LTD Total Cost in separate columns&#xA; --1. LTD amount&#xA; SELECT&#xA;  rtrim(A.PROJECT_ID) [Project]&#xA;  ,rtrim(A.CHARTFIELD1) [Subproject]&#xA;  , A.FISCAL_YEAR&#xA;  , A.ACCOUNTING_PERIOD&#xA;  , A.POSTED_BASE_AMT [1LTD]&#xA;  , '0' [2AM]&#xA;  , '0' [3Disposal]&#xA;  , '0' [4Transfers]&#xA;  , '0' [5Accruals]&#xA;  , '0' [6Prepaids]&#xA;  , rtrim(A.ACCOUNT) [Account]&#xA;  , null [AM Distribution Type]&#xA; into ##PID_Balances --***&#xA; FROM&#xA;  PS_LEDGER A&#xA; WHERE &#xA;  ( A.BUSINESS_UNIT = '10010'&#xA;  AND A.ACCOUNT IN (&#xA;  '0387000','0387010' --from original PS Query VC_GL_COA_CIP_AM_PID_COSTS&#xA;