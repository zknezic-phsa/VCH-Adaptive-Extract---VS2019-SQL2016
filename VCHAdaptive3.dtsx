                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--Updated 2023/05/03
	--Select statement for Import
declare @fy as varchar(4)
declare @per as varchar(2)
--declare @PID as varchar(6) --for testing

SET @fy = 
	(select FISCAL_YEAR from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in
	(select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))
SET @per =
	(select ACCOUNTING_PERIOD from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in
	(select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))
select
	'LTD_Total_Cost' as [Account] --hardcoded value
	,A1.[Level Code] as [Level Code]
	,A.Project as [Capital_Project Code]
	,A.Subproject as [Capital_Subproject Code]
	,convert(varchar,A3.END_DT,101) [Date] --period end date required for import
	,Sum(A.[1LTD])+Sum(A.[2AM])+Sum(A.[3Disposal])+Sum(A.[4Transfers])+Sum(A.[5Accruals])+Sum(A.[6Prepaids]) as [Value]
from ##PID_Balances A
	inner join ##PID_Dummies A1 on A.Project = A1.[Project Code] --use left join instead of inner join to capture any PIDs with activities but not in the tree VCH_CAP_REGIONAL
	left join PS_CAL_DETP_VW A3
		on A3.SETID = 'SET00'
		and A3.CALENDAR_ID = '02'
		and A.FISCAL_YEAR = A3.FISCAL_YEAR
		and A.ACCOUNTING_PERIOD = A3.ACCOUNTING_PERIOD
where
	A.Project &lt;&gt; ''
	And (
		(A.FISCAL_YEAR = @fy and A.ACCOUNTING_PERIOD = @per)
		OR
		(A.FISCAL_YEAR = case when @per = '1' then @fy -1 else @fy end and A.ACCOUNTING_PERIOD = case when @per = '1' then '13' else @per-1 end)
		) --2023/05/02 to properly include prior period at P1; from FAA-8975 Operating query "6_actuals_rev_and_nonlabour.csv"
	/*and A.FISCAL_YEAR = @fy
	and	A.ACCOUNTING_PERIOD between 1 and @per --to capture activities since P1 in case PIDs are added later to the VCH_CAP_REGIONAL tree
	*/
group by
	A1.[Level Code]
	,A.Project
	,A.Subproject
	,convert(varchar,A3.END_DT,101)
order by 2,3,4, 5</property>
