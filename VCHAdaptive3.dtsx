/***********************
4.3.2.4.	FILE REF NO. 7 – WORKFORCE ACTUALS LOAD (LDR)
Filename: 7_actuals_workforce.csv
Data Source: FSCM
Original Story: FAA-8975
Updated 2023/08/23
***********************/

declare @bu as varchar(5)
declare @fiscal as VARCHAR(4)
declare @completed_period as VARCHAR(2)
SET @bu = '10010'
SET @fiscal = 
	(select FISCAL_YEAR from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in
	(select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))
SET @completed_period = 
	(select ACCOUNTING_PERIOD from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in
	(select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))

select
	B.[Level Code] --Field 1 in Adaptive template
	,B.[Sum_Code] [WFSummary Account] --Field 2 in Adaptive template
	,B.[Job_Code Code] [Workforce_Job_Code Code] --Field 3 in Adaptive template
	,B.[Date] --Field 4 in Adaptive template
	,Sum(B.[Value]) [Value] --Field 5 in Adaptive template
	
from
	(--the following subqueries faciliate combining results from the monetary and hours columns into the same Value column.
		(--LDR Actuals Dollars
			SELECT
			rtrim(A.PH_LD_SUM_CODE)+'_'+replace(replace(rtrim(A1.DESCR),' ','_'),'&amp;','and')+'_'+'Dollars' as [Sum_Code]
			,rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT) as [Level Code]
			,right('000000'+cast(rtrim(A.JOBCODE)as varchar(6)),6) as [Job_Code Code] --need to zero-pad to compensate for manual adjustment inconsistencies
			,replace(rtrim(A2.DESCR),',',' ') as [Job_Code Name] --need to strip out commas for csv conversion to Excel
			,rtrim(A.ACCOUNT) as [GL Account] --for testing
			,convert(varchar,A3.END_DT,101) [Date]
			,BUSINESS_UNIT --for testing
			,A.FISCAL_YEAR --for testing
			,A.ACCOUNTING_PERIOD --for testing
			,sum(A.PH_LD_ACT_AMT) as [Value]
			FROM
			PS_PH_LD_REPORT A
			LEFT JOIN PS_PH_LD_SUM_CODE A1
				ON A.BUSINESS_UNIT = A1.SETID AND A.PH_LD_SUM_CODE = A1.PH_LD_SUM_CODE
			LEFT JOIN PS_PH_LD_JOB_CLASS A2
				ON A.BUSINESS_UNIT = A2.SETID AND A.JOBCODE = A2.PH_LD_JOB_CODE
				--select count(distinct PH_LD_JOB_CODE) from PS_PH_LD_JOB_CLASS WHERE SETID = '10010'
			left join PS_CAL_DETP_VW A3
				on A3.SETID = 'SET00'
				and A3.CALENDAR_ID = '02'
				and A.FISCAL_YEAR = A3.FISCAL_YEAR
				and A.ACCOUNTING_PERIOD = A3.ACCOUNTING_PERIOD
			WHERE
				A.BUSINESS_UNIT = @bu
				and
					(A.FISCAL_YEAR = (case when @completed_period &lt;= '2' then @fiscal -1 else @fiscal end)
						and A.ACCOUNTING_PERIOD = (case when @completed_period &lt;= '2' then 13 else @completed_period - 1 end)) or
					(A.FISCAL_YEAR = @fiscal and A.ACCOUNTING_PERIOD = @completed_period) or
					(A.FISCAL_YEAR = @fiscal and A.ACCOUNTING_PERIOD = @completed_period -1)
					--FAA-16138 2023/08/23 include prior period plus prior year P13 if current period is P1 or P2
			GROUP BY
				rtrim(A.PH_LD_SUM_CODE)+'_'+replace(replace(rtrim(A1.DESCR),' ','_'),'&amp;','and')+'_'+'Dollars'
				,rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT)
				,rtrim(A.JOBCODE)
				,rtrim(A2.DESCR)
				,rtrim(A.ACCOUNT)
				,convert(varchar,A3.END_DT,101)
				,BUSINESS_UNIT
				,A.FISCAL_YEAR
				,A.ACCOUNTING_PERIOD)
		UNION ALL
		(--LDR Actuals Hours
			SELECT
			rtrim(A.PH_LD_SUM_CODE)+'_'+replace(replace(rtrim(A1.DESCR),' ','_'),'&amp;','and')+'_'+'Hours' as [Sum_Code]
			,rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT) as [Level Code]
			,right('000000'+cast(rtrim(A.JOBCODE)as varchar(6)),6) as [Job_Code Code] --need to zero-pad to compensate for manual adjustment inconsistencies
			,replace(rtrim(A2.DESCR),',',' ') as [Job_Code Name] --need to strip out commas for csv conversion to Excel
			,rtrim(A.ACCOUNT) as [GL Account] --for testing
			,convert(varchar,A3.END_DT,101) [Date]
			,BUSINESS_UNIT --for testing
			,A.FISCAL_YEAR --for testing
			,A.ACCOUNTING_PERIOD --for testing
			,sum(A.PH_LD_ACT_HRS) as [Value]
			FROM
			PS_PH_LD_REPORT A
			LEFT JOIN PS_PH_LD_SUM_CODE A1
				ON A.BUSINESS_UNIT = A1.SETID AND A.PH_LD_SUM_CODE = A1.PH_LD_SUM_CODE
			LEFT JOIN PS_PH_LD_JOB_CLASS A2
				ON A.BUSINESS_UNIT = A2.SETID AND A.JOBCODE = A2.PH_LD_JOB_CODE
				--select count(distinct PH_LD_JOB_CODE) from PS_PH_LD_JOB_CLASS WHERE SETID = '10010'
			left join PS_CAL_DETP_VW A3
				on A3.SETID = 'SET00'
				and A3.CALENDAR_ID = '02'
				and A.FISCAL_YEAR = A3.FISCAL_YEAR
				and A.ACCOUNTING_PERIOD = A3.ACCOUNTING_PERIOD
			WHERE
				A.BUSINESS_UNIT = @bu
				and (
					(A.FISCAL_YEAR = (case when @completed_period &lt;= '2' then @fiscal -1 else @fiscal end)
						and A.ACCOUNTING_PERIOD = (case when @completed_period &lt;= '2' then 13 else @completed_period - 1 end)) or
					(A.FISCAL_YEAR = @fiscal and A.ACCOUNTING_PERIOD = @completed_period) or
					(A.FISCAL_YEAR = @fiscal and A.ACCOUNTING_PERIOD = @completed_period -1))
					--FAA-16138 2023/08/23 include prior period plus prior year P13 if current period is P1 or P2
			GROUP BY
				rtrim(A.PH_LD_SUM_CODE)+'_'+replace(replace(rtrim(A1.DESCR),' ','_'),'&amp;','and')+'_'+'Hours'
				,rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT)
				,rtrim(A.JOBCODE)
				,rtrim(A2.DESCR)
				,rtrim(A.ACCOUNT)
				,convert(varchar,A3.END_DT,101)
				,BUSINESS_UNIT
				,A.FISCAL_YEAR
				,A.ACCOUNTING_PERIOD)
	) B
	WHERE
	ABS([Value]) &lt;&gt; 0
	Group by
		B.[Level Code]
		,B.[Sum_Code]
		,B.[Job_Code Code]
		,FISCAL_YEAR
		,ACCOUNTING_PERIOD
		,B.[Date]
	order by FISCAL_YEAR,ACCOUNTING_PERIOD, 4,1,2,3</property>
