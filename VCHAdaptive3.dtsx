4.3.2.1.	FILE REF NO. 20 - DIMENSION - PROJECT ID
Filename: 20_dimensions_project.csv
Data Source: FSCM
Original story: FAA-8589
Updated: 2023/05/02
****************/
--ZKnezic: rewritten to use CTE instead of temp table
WITH PID_Dummies1 AS (
	select
		rtrim(B.TREE_NODE) [Level Code]
		,A.EFFDT
		,rtrim(B1.DESCR) [Level Name]
		,rtrim(A.RANGE_FROM) [Project Code]
		,rtrim(A1.DESCR) [Project Name]
	--into #PID_Dummies1
	from PSTREELEAF A
	left join PSTREENODE B on A.SETID = B.SETID and A.TREE_NODE_NUM = B.TREE_NODE_NUM
		and B.EFFDT = (select max(B_ED.EFFDT) from PSTREENODE B_ED where B.SETID = B_ED.SETID and A.TREE_NAME = B.TREE_NAME and B.TREE_NODE_NUM = B_ED.TREE_NODE_NUM and B_ED.EFFDT &lt;= A.EFFDT)
	left join PS_TREE_NODE_TBL B1 on B.SETID = B1.SETID and B.TREE_NODE = B1.TREE_NODE
	left join PS_PROJECT A1 on A.SETID = A1.BUSINESS_UNIT and A.RANGE_FROM = A1.PROJECT_ID
	where
		A.SETID = '10010'
		and A.TREE_NAME = 'VCH_CAP_REGIONAL'
		and A.EFFDT = (select max(A_ED.EFFDT) from PSTREELEAF A_ED where B.SETID = A_ED.SETID and A.TREE_NAME = A_ED.TREE_NAME and A.TREE_NODE_NUM = A_ED.TREE_NODE_NUM and A_ED.EFFDT &lt;=
		(select max(END_DT) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt;= getdate())) --2023/05/02 to take the EFFDT between the max date and today's date
	/*order by 4*/
	)

--select query

SELECT
	rtrim(A.PROJECT_ID) as [Capital_Project Code]
	, (case when  A.EFF_STATUS = 'I' then rtrim(A.DESCR) + ' *INACTIVATED' else  rtrim(A.DESCR) end) as 'Capital_Project Name'
	, isnull(rtrim(B.PH_NODE_LVL1_DESCR),'') as [Capital_ProjectType]
	, isnull(rtrim(B.PH_NODE_LVL2_DESCR),'') as [Capital_Project_Category]
	, case when B.PH_NODE_LVL3_DESCR = B.PH_NODE_LVL2_DESCR then '' --Tree View duplicates the higher level if none exist, instead Business wants this to be blank for Adaptive.
		else isnull(rtrim(B.PH_NODE_LVL3_DESCR),'') end as [Capital_Project_SubCategory]
	--, B.EFFDT --for testing
FROM
	PS_PROJECT A
	left join PS_PH_TR_VCAP_PROJ B
		on A.PROJECT_ID = B.PROJECT_ID
		AND B.EFFDT =
			(select max(B_ED.EFFDT) from PS_PH_TR_VCAP_PROJ B_ED where B.SETID = B_ED.SETID and B_ED.TREE_NAME = 'VCH_CAP_PROJECT'
			and B_ED.EFFDT &lt;= (select max(END_DT) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate()))
	inner join PID_Dummies1 A1 on A.PROJECT_ID = A1.[Project Code] --(added on 2023/04/21 update, copied from 4.3.2.2) use left join instead of inner join to capture any PIDs with activities but not in the tree VCH_CAP_REGIONAL
WHERE
	A.BUSINESS_UNIT = '10010'
	and A.PROJECT_ID not in ('04/01/2014','04/01/2017') --to hardcode-out erroneously created and unremovable PIDs in Psoft
order by 1</property>
