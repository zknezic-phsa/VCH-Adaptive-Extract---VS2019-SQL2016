--This subquery looks at the FHA_ACCOUNT tree for P&amp;L accounts
Select
CAST(rtrim(ACCOUNT)as NVARCHAR(30)) [GL Account]
,CAST(rtrim(PH_NODE_LVL4) AS NVARCHAR(50)) [Level 4 Account Code]
,CAST(rtrim(PH_NODE_LVL4_DESCR) AS NVARCHAR(100)) [Level 4 Account Name]
from
	PS_PH_ACCT_VCH_FH A
where
	SETID = 'SET00'
	and TREE_NAME = 'FHA_ACCOUNT'
	and ACCOUNT not like '0%'
	and EFFDT &lt;= (select (max(A_ED.EFFDT)) from PS_PH_TR_CC_VCH_FH A_ED where A.SETID = A_ED.SETID and A.TREE_NAME = A_ED.TREE_NAME and EFFDT &lt;= --2023/02/10 ensure only one tree is returned if the PS_PH_TR_CC_VCH_RG view contains more than one effective date
		(select max(END_DT) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate())) --2023/02/10 to ensure P13 uses the correct FY tree

union all
--This subquery looks at the FHA_ACCOUNT tree for B/S accounts
Select
CAST(rtrim(ACCOUNT)as NVARCHAR(30)) [GL Account]
,CAST(rtrim(PH_NODE_LVL4) AS NVARCHAR(50)) [Level 4 Account Code]
,CAST(rtrim(PH_NODE_LVL4_DESCR) AS NVARCHAR(100)) [Level 4 Account Name]
from
	PS_PH_ACCT_VCH_FH B
where
	SETID = 'SET00'
	and TREE_NAME = 'FHA_ACCOUNT_PSAB'
	and ACCOUNT like '0%'
	and EFFDT &lt;= (select (max(B_ED.EFFDT)) from PS_PH_TR_CC_VCH_FH B_ED where B.SETID = B_ED.SETID and B.TREE_NAME = B_ED.TREE_NAME and EFFDT &lt;= --2023/02/10 ensure only one tree is returned if the PS_PH_TR_CC_VCH_RG view contains more than one effective date
		(select max(END_DT) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate())) --2023/02/10 to ensure P13 uses the correct FY tree

