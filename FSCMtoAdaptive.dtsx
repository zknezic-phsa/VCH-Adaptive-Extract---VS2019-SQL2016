declare @bu as nvarchar(5)
SET @bu = '10010'

Select
CAST(rtrim(PH_DEPTSITE) as NVARCHAR(30)) [Cost Center]
,CAST(case
	when PH_NODE_LVL4 = '' and PH_NODE_LVL3 = '' and PH_NODE_LVL2 = ''  then rtrim(PH_NODE_LVL1)
	when PH_NODE_LVL4 = '' and PH_NODE_LVL3 = '' and PH_NODE_LVL2 &lt;&gt; ''  then rtrim(PH_NODE_LVL2)
	when PH_NODE_LVL4 = '' and PH_NODE_LVL3 &lt;&gt; '' and PH_NODE_LVL2 &lt;&gt; '' then rtrim(PH_NODE_LVL3)
	else rtrim(PH_NODE_LVL4)
	end AS NVARCHAR(50)) [Level 4 Program Code]
,CAST(case
	when PH_NODE_LVL4_DESCR = '' and PH_NODE_LVL3_DESCR = '' and PH_NODE_LVL2_DESCR = ''  then rtrim(PH_NODE_LVL1_DESCR)
	when PH_NODE_LVL4_DESCR = '' and PH_NODE_LVL3_DESCR = '' and PH_NODE_LVL2_DESCR &lt;&gt; ''  then rtrim(PH_NODE_LVL2_DESCR)
	when PH_NODE_LVL4_DESCR = '' and PH_NODE_LVL3_DESCR &lt;&gt; '' and PH_NODE_LVL2_DESCR &lt;&gt; '' then rtrim(PH_NODE_LVL3_DESCR)
	else rtrim(PH_NODE_LVL4_DESCR)
	end  AS NVARCHAR(50)) [Level 4 Program Name]
from
	PS_PH_TR_CC_VCH_RG A
where
	SETID = @bu
	and EFFDT &lt;= (select (max(A_ED.EFFDT)) from PS_PH_TR_CC_VCH_RG A_ED where A.SETID = A_ED.SETID and A.TREE_NAME = A_ED.TREE_NAME and EFFDT &lt;= --2023/02/10 ensure only one tree is returned if the PS_PH_TR_CC_VCH_RG view contains more than one effective date
	(select max(END_DT) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate())) --2023/02/10 to ensure P13 uses the correct FY tree
