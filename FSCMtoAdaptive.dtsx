                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">declare @bu as varchar(5)
declare @fiscal as VARCHAR(4)
declare @completed_period as VARCHAR(2)
SET @bu = '10010'
SET @fiscal = 
	(select FISCAL_YEAR from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in
	(select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))
SET @completed_period =
	(select ACCOUNTING_PERIOD from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in
	(select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))

select
	B.[Level Code] --Field 1 in Adaptive template
	,B.[Account] --Field 2 in Adaptive template
	,B.[Date] --Field 3 in Adaptive template
	,Sum(B.[Value]) [Value] --Field 4 in Adaptive template
	--,Sum(B.[DR/CR]) [DR/CR] --for testing
	--,B.[REV/EXP] --for testing
from
(--the following subqueries are needed to faciliate reversing the signs of only the revenue subset of lines
	(--Actuals Monetary P&amp;L EXPENSES (no sign change)
		SELECT
			rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT) as [Level Code]
			,rtrim(A.ACCOUNT) as [Account]
			,convert(varchar,A3.END_DT,101) [Date]
			,BUSINESS_UNIT --for testing
			,A.FISCAL_YEAR --for testing
			,A.ACCOUNTING_PERIOD --for testing
			,'EXP' [REV/EXP] --for testing
			,round(SUM( A.POSTED_TOTAL_AMT),2) as [Value]
			,round(SUM( A.POSTED_TOTAL_AMT),2) as [DR/CR] --for testing
		FROM PS_LEDGER A
		left join PS_PROGRAM_ALL_VW A2
			ON A.BUSINESS_UNIT = A2.SETID
			and A.PROGRAM_CODE = A2.PROGRAM_CODE
		left join PS_CAL_DETP_VW A3
			on A3.SETID = 'SET00'
			and A3.CALENDAR_ID = '02'
			and A.FISCAL_YEAR = A3.FISCAL_YEAR
			and A.ACCOUNTING_PERIOD = A3.ACCOUNTING_PERIOD
		WHERE
			A.BUSINESS_UNIT = @bu
			AND A.FISCAL_YEAR = @fiscal
			and (A.ACCOUNTING_PERIOD = @completed_period
			OR A.ACCOUNTING_PERIOD = @completed_period -1) --include prior period to ensure period close is captured
			AND A.CURRENCY_CD = 'CAD'
			AND A.STATISTICS_CODE = ''
			and A.ACCOUNT not like 'S%'
			and A.ACCOUNT not like '0%'
			and (A.ACCOUNT not like '1%' or A.ACCOUNT like '15015%' or A.ACCOUNT = '1222000') --2022/10/28
		GROUP BY
			rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT)
			,rtrim(A.ACCOUNT)
			,convert(varchar,A3.END_DT,101)
			,BUSINESS_UNIT
			,A.FISCAL_YEAR
			,A.ACCOUNTING_PERIOD
		HAVING
			SUM( A.POSTED_TOTAL_AMT) &lt;&gt; 0)
		--Actuals Monetary P&amp;L REVENUES signs reversed --2022/10/28
		UNION ALL
		
		(SELECT
			rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT) as [Level Code]
			,rtrim(A.ACCOUNT) as [Account]
			,convert(varchar,A3.END_DT,101) [Date]
			,BUSINESS_UNIT --for testing
			,A.FISCAL_YEAR --for testing
			,A.ACCOUNTING_PERIOD --for testing
			,'REV' [REV/EXP] --for testing
			,round(SUM( A.POSTED_TOTAL_AMT)*-1,2) as [Value]
			,round(SUM( A.POSTED_TOTAL_AMT),2) as [DR/CR] --for testing
		FROM PS_LEDGER A
		left join PS_PROGRAM_ALL_VW A2
			ON A.BUSINESS_UNIT = A2.SETID
			and A.PROGRAM_CODE = A2.PROGRAM_CODE
		left join PS_CAL_DETP_VW A3
			on A3.SETID = 'SET00'
			and A3.CALENDAR_ID = '02'
			and A.FISCAL_YEAR = A3.FISCAL_YEAR
			and A.ACCOUNTING_PERIOD = A3.ACCOUNTING_PERIOD
		WHERE
			A.BUSINESS_UNIT = @bu
			AND A.FISCAL_YEAR = @fiscal
			and (A.ACCOUNTING_PERIOD = @completed_period
			OR A.ACCOUNTING_PERIOD = @completed_period -1) --include prior period to ensure period close is captured
			AND A.CURRENCY_CD = 'CAD'
			AND A.STATISTICS_CODE = ''
			and A.ACCOUNT not like 'S%'
			and A.ACCOUNT not like '0%'
			and (A.ACCOUNT like '1%' and A.ACCOUNT not like '15015%' and A.ACCOUNT &lt;&gt; '1222000')--2022/10/28
		GROUP BY
			rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT)
			,rtrim(A.ACCOUNT)
			,convert(varchar,A3.END_DT,101)
			,BUSINESS_UNIT
			,A.FISCAL_YEAR
			,A.ACCOUNTING_PERIOD
		HAVING
			SUM( A.POSTED_TOTAL_AMT) &lt;&gt; 0)

		UNION ALL

		--Actuals Monetary B/S (for testing: Expenses + Revenue (original signs) + B/S = zero)
		(SELECT
			rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT) as [Level Code]
			,rtrim(A.ACCOUNT) as [Account]
			,convert(varchar,A3.END_DT,101) [Date]
			,BUSINESS_UNIT --for testing
			,A.FISCAL_YEAR --for testing
			,A.ACCOUNTING_PERIOD --for testing
			,'BS' [REV/EXP] --for testing
			,0 as [Value]
			,round(SUM( A.POSTED_TOTAL_AMT),2) as [DR/CR] --for testing
		FROM PS_LEDGER A
		left join PS_PROGRAM_ALL_VW A2
			ON A.BUSINESS_UNIT = A2.SETID
			and A.PROGRAM_CODE = A2.PROGRAM_CODE
		left join PS_CAL_DETP_VW A3
			on A3.SETID = 'SET00'
			and A3.CALENDAR_ID = '02'
			and A.FISCAL_YEAR = A3.FISCAL_YEAR
			and A.ACCOUNTING_PERIOD = A3.ACCOUNTING_PERIOD
		WHERE
			A.BUSINESS_UNIT = @bu
			AND A.FISCAL_YEAR = @fiscal
			and (A.ACCOUNTING_PERIOD = @completed_period
			OR A.ACCOUNTING_PERIOD = @completed_period -1) --include prior period to ensure period close is captured
			AND A.CURRENCY_CD = 'CAD'
			AND A.STATISTICS_CODE = ''
			and A.ACCOUNT like '0%'
		GROUP BY
			rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT)
			,rtrim(A.ACCOUNT)
			,convert(varchar,A3.END_DT,101)
			,BUSINESS_UNIT
			,A.FISCAL_YEAR
			,A.ACCOUNTING_PERIOD
		HAVING
			SUM( A.POSTED_TOTAL_AMT) &lt;&gt; 0)
) B
WHERE
	ABS([Value]) &lt;&gt; 0
Group by 
	B.[Level Code]
	,B.[Account]
	,B.[Date]
	,B.[REV/EXP]
order by 3,1,2</property>
