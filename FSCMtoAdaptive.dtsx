4.3.2.9.	FILE REF NO. 16 – ACTUALS ACTIVITES AND PRODUCTIVITY METRICS 
Filename: 16_actuals_activities.csv
Data source: FSCM
Original story: FAA-7221, FAA_7904
Modified: 2023/01/29
***********************/

declare @bu as varchar(5)
declare @fiscal as VARCHAR(4)
declare @completed_period as VARCHAR(2)
SET @bu = '10010'
SET @fiscal = 
	(select FISCAL_YEAR from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in
	(select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))
SET @completed_period = 
	(select ACCOUNTING_PERIOD from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and PERIOD_CTR in
	(select max(PERIOD_CTR) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate() and BEGIN_DT &lt; getdate()))

--Actuals Stats 
WITH stats_ledger AS
	(SELECT
		rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT) as [Level Code]
		,rtrim(A.ACCOUNT) as [Account]
		,case when A.[PROGRAM_CODE] ='' then 'Subaccount' else A.[PROGRAM_CODE] end [Subaccount]
		,convert(varchar,A3.END_DT,101) [Date]
		,BUSINESS_UNIT
		,A.FISCAL_YEAR
		,A.ACCOUNTING_PERIOD
		--,'STAT' [REV/EXP] --for testing
		,round(SUM( A.POSTED_TOTAL_AMT),2) as [Value]
		--,round(SUM( A.POSTED_TOTAL_AMT),2) as [DR/CR] --for testing
	FROM PS_LEDGER A
	left join PS_PROGRAM_ALL_VW A2
		ON A.BUSINESS_UNIT = A2.SETID
		and A.PROGRAM_CODE = A2.PROGRAM_CODE
	left join PS_CAL_DETP_VW A3
		on A3.SETID = 'SET00'
		and A3.CALENDAR_ID = '02'
		and A.FISCAL_YEAR = A3.FISCAL_YEAR
		and A.ACCOUNTING_PERIOD = A3.ACCOUNTING_PERIOD
	WHERE
		A.BUSINESS_UNIT = @bu
		and (A.FISCAL_YEAR &gt;= @fiscal -1) --include entire year to ensure retroactive adjustments is captured
		and A.ACCOUNTING_PERIOD between 1 and 13
		AND A.CURRENCY_CD = ''
		AND (A.STATISTICS_CODE &lt;&gt; 'HRS' OR A.STATISTICS_CODE is null)
		and A.ACCOUNT like 'S%'
	GROUP BY
		rtrim(A.DEPTID) + rtrim(A.OPERATING_UNIT)
		,rtrim(A.ACCOUNT)
		,A.PROGRAM_CODE
		,convert(varchar,A3.END_DT,101)
		,BUSINESS_UNIT
		,A.FISCAL_YEAR
		,A.ACCOUNTING_PERIOD
	HAVING
		SUM( A.POSTED_TOTAL_AMT) &lt;&gt; 0
	)
select
	[Level Code] --Field 1 in Adaptive template
	,[Account] --Field 2 in Adaptive template
	,[Subaccount] [Subaccount Code] --Field 3 in Adaptive template
	,[Date] --Field 4 in Adaptive template
	,Sum([Value]) [Value] --Field 5 in Adaptive template
from stats_ledger
WHERE
	ABS([Value]) &lt;&gt; 0
	and (
	([FISCAL_YEAR] = @fiscal and [ACCOUNTING_PERIOD] &lt;= @completed_period) --select all completed periods in current Fiscal
	or ([FISCAL_YEAR] = (case when @completed_period &lt;=3 then @fiscal -1 else @fiscal end)) --select entire prior Fiscal when currently completed up to P3 or earlier
	)
Group by 
	[Level Code]
	,[Account]
	,[Subaccount]
	,[Date]
	--,[REV/EXP]
order by 4,1,2,3</property>
