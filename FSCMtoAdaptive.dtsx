          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{7A5FF3AF-1FC2-4644-9F71-E7E0A1B3F89D}"
              SQLTask:SqlStatementSource="--Temporary tables for 5_LEVELS&#xA;--Temporary tables for 5_LEVELS&#xA;declare @bu as varchar(5)&#xA;declare @tree as varchar(20)&#xA;declare @fiscal as VARCHAR(4)&#xA;SET @bu = '10010' --for PHC set to '20010'; note that some of the following lines are VCH-specific and would need to be rewritten for PHC&#xA;SET @tree = 'CC_VCH_REGIONAL' --for PHC set to 'PHC_PGM_ROLLUP'&#xA;&#xA;IF OBJECT_ID(N'tempdb..##combo_valid') IS NOT NULL&#xA;BEGIN&#xA;DROP TABLE ##combo_valid&#xA;END&#xA;&#xA;IF OBJECT_ID(N'tempdb..##levels_R1') IS NOT NULL&#xA;BEGIN&#xA;DROP TABLE ##levels_R1&#xA;END&#xA;&#xA;&#xA;--Valid CC list used to add &quot;*CLOSED&quot; to invalid CCs in the results&#xA;&#x9;select&#xA;&#x9;rtrim(DEPTID)+rtrim(OPERATING_UNIT) [PH_DEPTSITE]&#xA;&#x9;into ##combo_valid&#xA;&#x9;from PS_PH_COMBO_HCM_VW where BUSINESS_UNIT = @bu&#xA;&#xA;--Release 1 SQL to select Level values at Cost Centre level&#xA;--SQL to select Level values at Node levels&#xA;--Note: top level is replaced with &quot;Vancouver Coastal Health' and is specific to CC_VCH_REGIONAL&#xA;select &#xA;&#x9;case when rtrim(D.TREE_NODE) = 'ALL COST CENTRE' then 'Vancouver Coastal Health' else replace(rtrim(D1.DESCR),',',' ') end [Name] --This is VCH-specific --Column A&#xA;&#x9;, case&#xA;&#x9;&#x9;when rtrim(D.PARENT_NODE_NAME) = 'ALL COST CENTRE' then 'VCH' --This is VCH-specific &#xA;&#x9;&#x9;when rtrim(D.TREE_NODE) = 'ALL COST CENTRE' then 'VCH_Consolidated' --This is VCH-specific &#xA;&#x9;&#x9;else rtrim(D.PARENT_NODE_NAME) end [Rolls Up To] --Column B&#xA;&#x9;, case when rtrim(D.TREE_NODE) = 'ALL COST CENTRE' then 'VCH' else rtrim(D.TREE_NODE) end [Code] --This is VCH-specific  --Column C&#xA;&#x9;,'' [Short Name] --Column D&#xA;&#x9;,'' [Description] --Column E&#xA;&#x9;,'No' [Excluded From Workflow] --Column F&#xA;&#x9;,'' [Benefit_Rate_Category Code] --Column G&#xA;&#x9;,'' [Benefit_Rate_Category Name] --Column H&#xA;&#x9;,'' [Department Code] --Column I&#xA;&#x9;,'' [Department Name] --Column J&#xA;&#x9;,'' [Labour_Category Code] --Column K&#xA;&#x9;,'' [Labour_Category Name] --Column L&#xA;&#x9;,'' [Metrics Code] --Column M&#xA;&#x9;,'' [Metrics Name] --Column N&#xA;&#x9;,'' [MIPS Level 1 Code] --Column O&#xA;&#x9;,'' [MIPS Level 1 Name] --Column P&#xA;&#x9;,'' [MIPS Level 2 Code] --Column Q&#xA;&#x9;,'' [MIPS Level 2 Name] --Column R&#xA;&#x9;,'' [MIPS Level 3 Code] --Column S&#xA;&#x9;,'' [MIPS Level 3 Name] --Column T&#xA;&#x9;,case when D.SETID = '20010' then 'Yes' else 'No' end [PHC Code] --Column U&#xA;&#x9;,'' [PHC Name] --Column V&#xA;&#x9;,'' [Revenue Offset Code] --Column W&#xA;&#x9;,'' [Revenue Offset Name] --Column X&#xA;&#x9;,'' [Sector Code] --Column Y&#xA;&#x9;,'' [Sector Name] --Column Z&#xA;&#x9;,'' [Site Code] --Column AA&#xA;&#x9;,'' [Site Name] --Column AB&#xA;into ##levels_R1&#xA;from PSTREENODE D&#xA;left join PS_TREE_NODE_TBL D1&#xA;&#x9;on D.SETID = D1.SETID and D.TREE_NODE = D1.TREE_NODE and D1.EFFDT = (select max(D1_ED.EFFDT) from PS_TREE_NODE_TBL D1_ED where D1.SETID=D1_ED.SETID and D1.TREE_NODE=D1_ED.TREE_NODE and EFF_STATUS = 'A')&#xA;where&#xA;&#x9;D.SETID = @bu&#xA;&#x9;and D.TREE_NAME = @tree&#xA;&#x9;and D.EFFDT = (select max(D_ED.EFFDT) from PSTREENODE D_ED where D.SETID=D_ED.SETID and D.TREE_NAME=D_ED.TREE_NAME&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;and D_ED.EFFDT &lt;= (select max(END_DT) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate())) --2023/02/10 to ensure P13 uses the correct FY tree&#xA;&#xA;union all&#xA;&#xA;--SQL to select Level values at Cost Centre level&#xA;select&#xA;&#x9;case when rtrim(A.RANGE_FROM) in (select * from ##combo_valid) and A.SETID = @bu then replace(rtrim(A2.DESCR),',',' ')+': '+left(rtrim(A.RANGE_FROM),8)+'-'+right(rtrim(A.RANGE_FROM),3)&#xA;&#x9;&#x9;else replace(rtrim(A2.DESCR),',',' ')+'*CLOSED: '+left(rtrim(A.RANGE_FROM),8)+'-'+right(rtrim(A.RANGE_FROM),3) --Adds &quot;*CLOSED&quot; to description if not in combo list&#xA;&#x9;end [Name]  --Column A&#xA;&#x9;,rtrim(A1.TREE_NODE) [Rolls Up To] --Column B&#xA;&#x9;,rtrim(A.RANGE_FROM) [Code] --Column C&#xA;&#x9;,rtrim(left(A.RANGE_FROM,8)) [Short Name] --Column D&#xA;&#x9;,'' [Description] --Column E&#xA;&#x9;,'No' [Excluded From Workflow] --Column F&#xA;&#x9;,'' [Benefit_Rate_Category Code] --Column G&#xA;&#x9;,'' [Benefit_Rate_Category Name] --Column H&#xA;&#x9;,rtrim(left(A.RANGE_FROM,8)) [Department Code] --Column I&#xA;&#x9;,rtrim(left(A.RANGE_FROM,8)) [Department Name] --Column J&#xA;&#x9;,'' [Labour_Category Code] --Column K&#xA;&#x9;,'' [Labour_Category Name] --Column L&#xA;&#x9;,'' [Metrics Code] --Column M&#xA;&#x9;,'' [Metrics Name] --Column N&#xA;&#x9;,isnull(rtrim(A3.PH_NODE_LVL1),'') [MIPS Level 1 Code] --Column O&#xA;&#x9;,isnull(rtrim(A3.PH_NODE_LVL1_DESCR),'') [MIPS Level 1 Name] --Column P&#xA;&#x9;,isnull(rtrim(A3.PH_NODE_LVL2),'') [MIPS Level 2 Code] --Column Q&#xA;&#x9;,isnull(rtrim(A3.PH_NODE_LVL2_DESCR),'') [MIPS Level 2 Name] --Column R&#xA;&#x9;,isnull(rtrim(A3.PH_NODE_LVL3),'') [MIPS Level 3 Code] --Column S&#xA;&#x9;,isnull(rtrim(A3.PH_NODE_LVL3_DESCR),'') [MIPS Level 3 Name] --Column T&#xA;&#x9;,case when A.SETID = '20010' then 'Yes' else 'No' end [PHC Code] --Column U&#xA;&#x9;,'' [PHC Name] --Column V&#xA;&#x9;,'' [Revenue Offset Code] --Column W&#xA;&#x9;,'' [Revenue Offset Name] --Column X&#xA;&#x9;,isnull(replace(rtrim(A4.PH_FSCM_SECTOR),'&amp;',''),'') [Sector Code] --Column Y&#xA;&#x9;,'' [Sector Name] --Column Z&#xA;&#x9;,rtrim(right(rtrim(A.RANGE_FROM),3)) [Site Code] --Column AA&#xA;&#x9;,'' [Site Name] --Column AB&#xA;from PSTREELEAF A&#xA;left join PSTREENODE A1&#xA;&#x9;on A.SETID = A1.SETID and A.TREE_NAME = A1.TREE_NAME and A.EFFDT = A1.EFFDT and A.TREE_NODE_NUM = A1.TREE_NODE_NUM&#xA;left join PS_DEPT_TBL A2&#xA;&#x9;on A.SETID = A2.SETID and left(rtrim(A.RANGE_FROM),8) = A2.DEPTID and A2.EFFDT = (select max(A2_ED.EFFDT) from PS_DEPT_TBL A2_ED&#x9;where A2.SETID = A2_ED.SETID and A2.DEPTID = A2_ED.DEPTID)&#xA;left join PS_PH_TR_MIPS_CC A3&#xA;&#x9;on A.SETID = A3.SETID and A3.EFFDT = (select max(A3_ED.EFFDT) from PS_PH_TR_MIPS_CC A3_ED where A3.SETID = A3_ED.SETID and A3.TREE_NAME = A3_ED.TREE_NAME) and A.RANGE_FROM = A3.PH_DEPTSITE&#xA;left join PS_PH_HCM_SECTOR A4&#xA;&#x9;on A.SETID = A4.SETID and A4.EFFDT = (select max(A4_ED.EFFDT) from PS_PH_HCM_SECTOR A4_ED where A4.SETID = A4_ED.SETID) and A.RANGE_FROM = A4.PH_DEPTSITE&#xA;where&#xA;&#x9;A.SETID = @bu&#xA;&#x9;and A.TREE_NAME = @tree&#xA;&#x9;and A.EFFDT =&#xA;&#x9;&#x9;(select max(A_ED.EFFDT) from PSTREELEAF A_ED where A.SETID = A_ED.SETID and A.TREE_NAME = A_ED.TREE_NAME&#xA;&#x9;&#x9;and A_ED.EFFDT &lt;= (select max(END_DT) from PS_CAL_DETP_VW where SETID = 'SET00' and CALENDAR_ID = '02' and END_DT &lt; getdate())) --2023/02/10 to ensure P13 uses the correct FY tree&#xA;&#xA;&#xA;&#x9;&#xA;--Release 2 template from Release 1 temp table&#xA;select&#xA;&#x9;Code [Level Code]&#xA;&#x9;,[Name] [Level Name]&#xA;&#x9;,[Rolls Up To] [Rolls Up To]&#xA;&#x9;--,'' [Benefit_Rate_Category Code] --not from PSFT&#xA;&#x9;,[Department Code] [Department Code]&#xA;&#x9;--,'' [Labour_Category Code] --not from PSFT&#xA;&#x9;,case when [MIPS Level 1 Code] = '' then '' else [MIPS Level 1 Code] end [MIPS_Level_1 Code]&#xA;&#x9;,case when [MIPS Level 2 Code] = '' then ''&#x9;else [MIPS Level 2 Code] end [MIPS_Level_2 Code]&#xA;&#x9;,case when [MIPS Level 3 Code] = '' then ''&#x9;else [MIPS Level 3 Code] end [MIPS_Level_3 Code]&#xA;&#x9;,[PHC Code] [PHC Code]&#xA;&#x9;--,'' [Revenue Offset Code] --not from PSFT&#xA;&#x9;,case when [Sector Code] = '' then '' else [Sector Code] end [Sector Code]&#xA;&#x9;,case when [Site Code] = '' then ''&#x9;else [Site Code] end [Site Code]&#xA;&#x9;--,'' [TF Initiative Code] --not from PSFT&#xA;&#x9;--,'' [TF Initiative Sub Category Code] --not from PSFT&#xA;from ##levels_R1&#xA;order by 3" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
      </DTS:Executables>
